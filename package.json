{
  "name": "ui-logon",
  "version": "1.4.0",
  "description": "A logon page in Angular and Node",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --build-optimizer",
    "ngbuild": "ng build logon --configuration production",
    "publish": "cd dist/logon && npm publish",
    "build_publish": "npm run ngbuild && npm run publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --dev-server-target= --webdriverUpdate=false",
    "redis": "redis-server /etc/redis/6379.conf",
    "prod": "node $NODE_DEBUG_OPTION server.js"
  },
  "dependencies": {
    "@angular/animations": "^12.0.2",
    "@angular/cdk": "^12.0.2",
    "@angular/common": "^12.0.2",
